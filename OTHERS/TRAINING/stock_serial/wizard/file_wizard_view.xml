<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="file_wizard_view_form" model="ir.ui.view">
            <field name="name">file.wizard.form</field>
            <field name="model">file.wizard</field>
            <field name="arch" type="xml">
                <form string="File">
                    <group>
                        <field name="file_lot"/>
                    </group>
                </form>
                <footer>
                    <button name="action_upload" string="Upload" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </field>
        </record>
    </data>
</odoo>