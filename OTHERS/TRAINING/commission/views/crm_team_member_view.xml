<odoo>
    <record id="crm_team_member_form" model="ir.ui.view">
        <field name="name">crm team member form</field>
        <field name="model">crm.team.member</field>
        <field name="inherit_id" ref="sales_team.crm_team_member_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='crm_team_id']" position="after">
                <field name="commission_plan_id"/>
                <field name="commission_amount"/>
            </xpath>
        </field>
    </record>
</odoo>
